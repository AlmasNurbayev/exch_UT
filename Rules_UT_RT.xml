				<СпособОтбораДанных>ПроизвольныйАлгоритм</СпособОтбораДанных>
				<ПередОбработкойПравила>
					ДатаПереносаОстатков = НачалоДня(Параметры.ДатаВыгрузкиОстатков)-1;

					Запрос = Новый Запрос;
					Запрос.УстановитьПараметр("ДатаВыгрузкиОстатков", ДатаПереносаОстатков);
					Запрос.УстановитьПараметр("Дисконтная", Перечисления.ТипыИнформационныхКарт.Дисконтная);
