				<Порядок>150</Порядок>
				<ПередВыгрузкой>
					Товары = Новый ТаблицаЗначений;
					Товары.Колонки.Добавить("Номенклатура");
					Товары.Колонки.Добавить("Упаковка");
					Товары.Колонки.Добавить("Количество");
					Товары.Колонки.Добавить("КоличествоУпаковок");
					Товары.Колонки.Добавить("Цена");
					Товары.Колонки.Добавить("Сумма");
					Товары.Колонки.Добавить("КлючСвязиСерийныхНомеров");

					СерийныеНомера = Новый ТаблицаЗначений;
					СерийныеНомера.Колонки.Добавить("КлючСвязиСерийныхНомеров");
					СерийныеНомера.Колонки.Добавить("СерийныйНомер");

					СуммаДокумента = 0;

					Если ТолькоПолучитьУзелСсылки &lt;&gt; Истина Тогда

					КлючСвязи = 0;

					Для Каждого СтрокаТабличнойЧасти Из Источник.Товары Цикл

					СтрокаТовары = Товары.Найти(СтрокаТабличнойЧасти.Номенклатура, "Номенклатура");

					Если СтрокаТовары = Неопределено Тогда
					КлючСвязи = КлючСвязи + 1;
					СтрокаТовары = Товары.Добавить();
					СтрокаТовары.Номенклатура = СтрокаТабличнойЧасти.Номенклатура;
					СтрокаТовары.Упаковка = СтрокаТабличнойЧасти.ЕдиницаИзмерения;
					СтрокаТовары.КоличествоУпаковок = 0;
					СтрокаТовары.Количество = 0;
					СтрокаТовары.Сумма = 0;
					Номинал = Параметры.ПодарочныеСертификаты.Получить(СтрокаТовары.Номенклатура);
					СтрокаТовары.Цена = ?(Номинал = Неопределено, 0, Номинал);
					СтрокаТовары.КлючСвязиСерийныхНомеров = КлючСвязи;
					КонецЕсли;

					СтрокаТовары.КоличествоУпаковок = СтрокаТовары.КоличествоУпаковок +
					СтрокаТабличнойЧасти.Количество;

					СтрокаТовары.Количество = СтрокаТовары.Количество + СтрокаТабличнойЧасти.Количество *
					СтрокаТабличнойЧасти.Коэффициент;

					СтрокаТовары.Сумма = СтрокаТовары.Сумма + СтрокаТовары.Цена *
					СтрокаТабличнойЧасти.Количество;

					СтрокаСерийныеНомера = СерийныеНомера.Добавить();
					СтрокаСерийныеНомера.КлючСвязиСерийныхНомеров = КлючСвязи;
					СтрокаСерийныеНомера.СерийныйНомер = СтрокаТабличнойЧасти.ХарактеристикаНоменклатуры;
					КонецЦикла;

