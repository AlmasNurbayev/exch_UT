					Запрос.УстановитьПараметр("ПустаяДата", Дата("00010101"));
					Запрос.Текст =
					"ВЫБРАТЬ
					| ВложенныйЗапрос.ВладелецДисконтнойКарты,
					| ВложенныйЗапрос.ДисконтнаяКарта,
					| ВложенныйЗапрос.Сумма,
					| ВложенныйЗапрос.ПериодОборот
					|ИЗ
					| (ВЫБРАТЬ
					| НакопленияПоДисконтнымКартамОбороты.Карта.ВладелецКарты КАК ВладелецДисконтнойКарты,
					| НакопленияПоДисконтнымКартамОбороты.Карта КАК ДисконтнаяКарта,
					| СУММА(НакопленияПоДисконтнымКартамОбороты.СуммаОборот) КАК Сумма,
					| МАКСИМУМ(НакопленияПоДисконтнымКартамОбороты.Период) КАК ПериодОборот
					| ИЗ
					| РегистрНакопления.НакопленияПоДисконтнымКартам.Обороты(
					| ,
					| &amp;ДатаВыгрузкиОстатков,
					| День,
					| (НЕ Карта.Запрещена)
					| И Карта.ТипКарты = &amp;Дисконтная
					| И Карта.ДатаЗапрета = &amp;ПустаяДата) КАК НакопленияПоДисконтнымКартамОбороты
					|
					| СГРУППИРОВАТЬ ПО
					| НакопленияПоДисконтнымКартамОбороты.Карта.ВладелецКарты,
					| НакопленияПоДисконтнымКартамОбороты.Карта) КАК ВложенныйЗапрос
					|ГДЕ
					| ВложенныйЗапрос.ПериодОборот &gt;= &amp;ДатаАктуальности
					|УПОРЯДОЧИТЬ ПО
					| ВложенныйЗапрос.ДисконтнаяКарта.Наименование";

					ДанныеДокумента = Новый Структура;
					ДанныеДокумента.Вставить("Дата", ДатаПереносаОстатков);
					ДанныеДокумента.Вставить("Комментарий", "###Ввод остатков Продажи по дисконтным картам");
					ТаблицаРегистров = Новый ТаблицаЗначений;
					ТаблицаРегистров.Колонки.Добавить("Имя");

