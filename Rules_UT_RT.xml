							<Приемник Имя="Количество" Вид="Реквизит" Тип="Число" />
							<ПередВыгрузкой>Значение = ОбъектКоллекции.Количество * ОбъектКоллекции.Коэффициент;</ПередВыгрузкой>
						</Свойство>
						<Свойство>
							<Код>2</Код>
							<Наименование>Количество --&gt; КоличествоУпаковок</Наименование>
							<Порядок>250</Порядок>
							<Источник Имя="Количество" Вид="Реквизит" Тип="Число" />
							<Приемник Имя="КоличествоУпаковок" Вид="Реквизит" Тип="Число" />
						</Свойство>
						<Свойство>
							<Код>5</Код>
							<Наименование>Цена --&gt; Цена</Наименование>
							<Порядок>300</Порядок>
							<Источник Имя="Цена" Вид="Реквизит" Тип="Число" />
							<Приемник Имя="Цена" Вид="Реквизит" Тип="Число" />
						</Свойство>
						<Свойство>
							<Код>4</Код>
							<Наименование>Сумма --&gt; Сумма</Наименование>
							<Порядок>350</Порядок>
							<Источник Имя="Сумма" Вид="Реквизит" Тип="Число" />
							<Приемник Имя="Сумма" Вид="Реквизит" Тип="Число" />
						</Свойство>
					</Группа>
					<Группа>
						<Код>20</Код>
						<Наименование>--&gt; СерийныеНомера</Наименование>
						<Порядок>650</Порядок>
						<Источник Имя="" Вид="" />
						<Приемник Имя="СерийныеНомера" Вид="ТабличнаяЧасть" />
						<ПередОбработкойВыгрузки>
							Если НЕ ВходящиеДанные.Свойство("СерийныеНомера", КоллекцияОбъектов) Тогда
							Отказ = Истина;
							КонецЕсли;</ПередОбработкойВыгрузки>
						<Свойство>
							<Код>21</Код>
							<Наименование>--&gt; КлючСвязиСерийныхНомеров</Наименование>
							<Порядок>50</Порядок>
							<Источник Имя="" Вид="" />
							<Приемник Имя="КлючСвязиСерийныхНомеров" Вид="Реквизит" Тип="Число" />
						</Свойство>
						<Свойство>
							<Код>22</Код>
							<Наименование>--&gt; СерийныйНомер</Наименование>
							<Порядок>100</Порядок>
							<Источник Имя="" Вид="" />
							<Приемник Имя="СерийныйНомер" Вид="Реквизит" Тип="СправочникСсылка.СерийныеНомера" />
							<КодПравилаКонвертации>ХарактеристикиНоменклатурыСерийныеНомера </КодПравилаКонвертации>
						</Свойство>
					</Группа>
				</Свойства>
				<Значения />
			</Правило>
			<Правило>
				<Код>ОтчетОРозничныхПродажах</Код>
				<Наименование>Документ: Отчет о розничных продажах</Наименование>
				<Порядок>250</Порядок>
				<ПередВыгрузкой>
					СуммаВозвратов = 0;
					СуммаОплатыНаличных = 0;
					ОплатаПодарочнымиСертификатами = 0;

					ВозвращенныеТовары = Новый ТаблицаЗначений;
					ВозвращенныеТовары.Колонки.Добавить("Номенклатура");
					ВозвращенныеТовары.Колонки.Добавить("Характеристика");
					ВозвращенныеТовары.Колонки.Добавить("Упаковка");
					ВозвращенныеТовары.Колонки.Добавить("Количество");
					ВозвращенныеТовары.Колонки.Добавить("КоличествоУпаковок");
					ВозвращенныеТовары.Колонки.Добавить("Цена");
					ВозвращенныеТовары.Колонки.Добавить("Сумма");
					ВозвращенныеТовары.Колонки.Добавить("ПроцентСкидкиНаценки");
					ВозвращенныеТовары.Колонки.Добавить("СтавкаНДС");
					ВозвращенныеТовары.Колонки.Добавить("СуммаНДС");
					ВозвращенныеТовары.Колонки.Добавить("ДисконтнаяКарта");
					ВозвращенныеТовары.Колонки.Добавить("Продавец");
					ВозвращенныеТовары.Колонки.Добавить("Склад");
					ВозвращенныеТовары.Колонки.Добавить("КлючСвязи");

					Товары = ВозвращенныеТовары.СкопироватьКолонки();
					Товары.Колонки.Добавить("КлючСвязиСерийныхНомеров");

					ОплатаПлатежнымиКартами = Новый ТаблицаЗначений;
					ОплатаПлатежнымиКартами.Колонки.Добавить("ВидОплаты");
					ОплатаПлатежнымиКартами.Колонки.Добавить("Сумма", Новый ОписаниеТипов("Число", Новый
					КвалификаторыЧисла(15, 2)));

					ПогашениеПодарочныхСертификатов = Новый ТаблицаЗначений;
					ПогашениеПодарочныхСертификатов.Колонки.Добавить("ПодарочныйСертификат");
					ПогашениеПодарочныхСертификатов.Колонки.Добавить("СерийныйНомер");
					ПогашениеПодарочныхСертификатов.Колонки.Добавить("Количество");


					СерийныеНомера = Новый ТаблицаЗначений;
					СерийныеНомера.Колонки.Добавить("КлючСвязиСерийныхНомеров");
					СерийныеНомера.Колонки.Добавить("СерийныйНомер");

					Если ТолькоПолучитьУзелСсылки &lt;&gt; Истина Тогда

					КлючСвязи = 0;

					Для Каждого СтрокаТабличнойЧасти Из Источник.Товары Цикл

					КлючСвязи = КлючСвязи + 1;

					СтрокаТаблицы = Товары.Добавить();
					СтрокаТаблицы.Номенклатура = СтрокаТабличнойЧасти.Номенклатура;
					СтрокаТаблицы.Характеристика = СтрокаТабличнойЧасти.ХарактеристикаНоменклатуры;
					СтрокаТаблицы.Упаковка = СтрокаТабличнойЧасти.ЕдиницаИзмерения;
					СтрокаТаблицы.Количество = СтрокаТабличнойЧасти.Количество *
					СтрокаТабличнойЧасти.Коэффициент;
					СтрокаТаблицы.КоличествоУпаковок = СтрокаТабличнойЧасти.Количество;
					СтрокаТаблицы.Цена = СтрокаТабличнойЧасти.Цена;
					СтрокаТаблицы.Сумма = СтрокаТабличнойЧасти.Сумма;
					СтрокаТаблицы.ПроцентСкидкиНаценки = Макс(СтрокаТабличнойЧасти.ПроцентСкидкиНаценки,
					СтрокаТабличнойЧасти.ПроцентАвтоматическихСкидок);
					СтрокаТаблицы.СтавкаНДС = СтрокаТабличнойЧасти.СтавкаНДС;
					СтрокаТаблицы.СуммаНДС = СтрокаТабличнойЧасти.СуммаНДС;
					Если ТипЗнч(СтрокаТабличнойЧасти.ЗначениеУсловияАвтоматическойСкидки) =
					Тип("СправочникСсылка.ИнформационныеКарты") Тогда
					СтрокаТаблицы.ДисконтнаяКарта = СтрокаТабличнойЧасти.ЗначениеУсловияАвтоматическойСкидки;
					Иначе
					СтрокаТаблицы.ДисконтнаяКарта = Справочники.ИнформационныеКарты.ПустаяСсылка();
					КонецЕсли;
					СтрокаТаблицы.Продавец = СтрокаТабличнойЧасти.Менеджер;
					СтрокаТаблицы.Склад = Источник.Склад;
					СтрокаТаблицы.КлючСвязи = КлючСвязи;

					Если (СтрокаТабличнойЧасти.Количество &lt; 0) Тогда
					СтрокаВозврат = ВозвращенныеТовары.Добавить();
					ЗаполнитьЗначенияСвойств(СтрокаВозврат, СтрокаТаблицы);
					СтрокаВозврат.Количество = -СтрокаВозврат.Количество;
					СтрокаВозврат.КоличествоУпаковок = -СтрокаВозврат.КоличествоУпаковок;
					СтрокаВозврат.Сумма = -СтрокаВозврат.Сумма;
					СтрокаВозврат.СуммаНДС = -СтрокаВозврат.СуммаНДС;
					КонецЕсли;

					КонецЦикла;

					СуммаВозвратов = ВозвращенныеТовары.Итог("Сумма");

					Запрос = Новый Запрос;
					Запрос.УстановитьПараметр("ТаблицаПодарочныеКарты", Источник.ПодарочныеКарты);
					Запрос.Текст =
					"ВЫБРАТЬ
					| ТаблицаПодарочныеКарты.НомерСтроки,
					| ТаблицаПодарочныеКарты.Карта
					|ПОМЕСТИТЬ ТаблицаПодарочныеКарты
					|ИЗ
					| &amp;ТаблицаПодарочныеКарты КАК ТаблицаПодарочныеКарты
					|;
					|
					|////////////////////////////////////////////////////////////////////////////////
					|ВЫБРАТЬ
					| ТаблицаПодарочныеКарты.НомерСтроки,
					| Штрихкоды.Владелец КАК Номенклатура,
					| Выразить(Штрихкоды.Владелец КАК Справочник.Номенклатура).СтавкаНДС КАК СтавкаНДС,
					| Штрихкоды.ХарактеристикаНоменклатуры,
					| Штрихкоды.ЕдиницаИзмерения
					|ИЗ
					| ТаблицаПодарочныеКарты КАК ТаблицаПодарочныеКарты
					| ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.Штрихкоды КАК Штрихкоды
					| ПО ТаблицаПодарочныеКарты.Карта.КодКарты = Штрихкоды.Штрихкод";

					ТаблицаПодарочныеКарты = Запрос.Выполнить().Выгрузить();


					Для Каждого СтрокаТабличнойЧасти Из Источник.ПодарочныеКарты Цикл
					СтрокаТаблицыПодарочныеКарты =
					ТаблицаПодарочныеКарты.Найти(СтрокаТабличнойЧасти.НомерСтроки, "НомерСтроки");

					Если СтрокаТаблицыПодарочныеКарты = Неопределено Тогда
					Продолжить;
					КонецЕсли;

					Если СтрокаТабличнойЧасти.ВидОперации = Перечисления.ОперацииСПодарочнымиКартами.Прием
					Тогда
					СтрокаТаблицы = ПогашениеПодарочныхСертификатов.Добавить();
					СтрокаТаблицы.ПодарочныйСертификат = СтрокаТаблицыПодарочныеКарты.Номенклатура;
					СтрокаТаблицы.СерийныйНомер = СтрокаТаблицыПодарочныеКарты.ХарактеристикаНоменклатуры;
					СтрокаТаблицы.Количество = 1;
					Иначе
					СтрокаТаблицы = Товары.Найти(СтрокаТаблицыПодарочныеКарты.Номенклатура, "Номенклатура");
					Если СтрокаТаблицы = Неопределено Тогда
					КлючСвязи = КлючСвязи + 1;
					СтрокаТаблицы = Товары.Добавить();
					СтрокаТаблицы.Номенклатура = СтрокаТаблицыПодарочныеКарты.Номенклатура;
					СтрокаТаблицы.Характеристика = Справочники.ХарактеристикиНоменклатуры.ПустаяСсылка();
					СтрокаТаблицы.Упаковка = СтрокаТаблицыПодарочныеКарты.ЕдиницаИзмерения;
					СтрокаТаблицы.СтавкаНДС = СтрокаТаблицыПодарочныеКарты.СтавкаНДС;
					Цена = Параметры.ПодарочныеСертификаты.Получить(СтрокаТаблицы.Номенклатура);
					Если Цена = Неопределено Тогда
					Цена = 0;
					КонецЕсли;
					СтрокаТаблицы.Цена = Цена;
					СтрокаТаблицы.Количество = 0;
					СтрокаТаблицы.КоличествоУпаковок = 0;
					СтрокаТаблицы.Сумма = 0;
					СтрокаТаблицы.СуммаНДС = 0;
					СтрокаТаблицы.Склад = Источник.Склад;
					СтрокаТаблицы.КлючСвязиСерийныхНомеров = КлючСвязи;
					СтрокаТаблицы.КлючСвязи = КлючСвязи;
					КонецЕсли;

					СтрокаТаблицы.Количество = СтрокаТаблицы.Количество + 1;
					СтрокаТаблицы.КоличествоУпаковок = СтрокаТаблицы.КоличествоУпаковок + 1;
					СтрокаТаблицы.Сумма = СтрокаТаблицы.Сумма + СтрокаТаблицы.Цена;
					РассчитатьСуммуНДСТабЧасти(СтрокаТаблицы, Источник);

					СтрокаТаблицы = СерийныеНомера.Добавить();
					СтрокаТаблицыКлючСвязи = Товары.Найти(СтрокаТаблицыПодарочныеКарты.Номенклатура,
					"Номенклатура");
					Если СтрокаТаблицыКлючСвязи &lt;&gt; Неопределено Тогда
					СтрокаТаблицы.КлючСвязиСерийныхНомеров = СтрокаТаблицыКлючСвязи.КлючСвязиСерийныхНомеров;
					Иначе
					СтрокаТаблицы.КлючСвязиСерийныхНомеров = КлючСвязи;
					КонецЕсли;
					СтрокаТаблицы.СерийныйНомер = СтрокаТаблицыПодарочныеКарты.ХарактеристикаНоменклатуры;
					КонецЕсли;

					КонецЦикла;

					Для Каждого СтрокаТабличнойЧасти Из Источник.Оплата Цикл

					Если СтрокаТабличнойЧасти.ВидОплаты = Справочники.ВидыОплатЧекаККМ.Терминал Тогда
					СтрокаТаблицы = ОплатаПлатежнымиКартами.Добавить();
					ЗаполнитьЗначенияСвойств(СтрокаТаблицы, СтрокаТабличнойЧасти);
					ИначеЕсли СтрокаТабличнойЧасти.ВидОплаты = Справочники.ВидыОплатЧекаККМ.Наличные Тогда
					Если СтрокаТабличнойЧасти.Сумма &lt;&gt; "" Тогда
					СуммаОплатыНаличных = СуммаОплатыНаличных + Число(СтрокаТабличнойЧасти.Сумма);
					КонецЕсли;
					ИначеЕсли СтрокаТабличнойЧасти.ВидОплаты = Справочники.ВидыОплатЧекаККМ.ПоПодарочнойКарте
					Тогда
					Если СтрокаТабличнойЧасти.Сумма &lt;&gt; "" Тогда
					ОплатаПодарочнымиСертификатами = ОплатаПодарочнымиСертификатами +
					Число(СтрокаТабличнойЧасти.Сумма);
					КонецЕсли;
					КонецЕсли;

					КонецЦикла;

					ОплатаПлатежнымиКартами.Свернуть("ВидОплаты", "Сумма");

					КонецЕсли;

					Если ТаблицаПодарочныеКарты.Количество() = 0 Тогда
					ЗапросДоп = Новый Запрос;
					ЗапросДоп.Текст =
					"ВЫБРАТЬ
					| ОтчетОРозничныхПродажахТовары.Номенклатура,
					| ОтчетОРозничныхПродажахТовары.ХарактеристикаНоменклатуры
					|ПОМЕСТИТЬ ВТ_Данные
					|ИЗ
					| Документ.ОтчетОРозничныхПродажах.Товары КАК ОтчетОРозничныхПродажахТовары
					| ГДЕ
					| ОтчетОРозничныхПродажахТовары.Ссылка = &amp;Ссылка
					| И ОтчетОРозничныхПродажахТовары.Номенклатура.Наименование ПОДОБНО ""%Сертификат%""
					|;
					|
					|////////////////////////////////////////////////////////////////////////////////
					|ВЫБРАТЬ
					| ВТ_Данные.Номенклатура,
					| ВТ_Данные.ХарактеристикаНоменклатуры,
					| Штрихкоды.Штрихкод
					|ИЗ
					| РегистрСведений.Штрихкоды КАК Штрихкоды
					| ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_Данные КАК ВТ_Данные
					| ПО Штрихкоды.Владелец = ВТ_Данные.Номенклатура
					| И Штрихкоды.ХарактеристикаНоменклатуры = ВТ_Данные.ХарактеристикаНоменклатуры";
					ЗапросДоп.УстановитьПараметр("Ссылка",Источник.Ссылка);
					ВыборкаДоп = ЗапросДоп.Выполнить().Выгрузить();
					Для Каждого ТекСтрока Из ВыборкаДоп Цикл
					СтрокаТаблицыКлючСвязи = Товары.НайтиСтроки(Новый
					Структура("Номенклатура,Характеристика",ТекСтрока.Номенклатура,ТекСтрока.ХарактеристикаНоменклатуры));
					Если СтрокаТаблицыКлючСвязи.Количество() &lt;&gt; 0 Тогда
