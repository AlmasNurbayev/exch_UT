		<Группа Отключить="true">
			<Код>ВводОстатков</Код>
			<Наименование>ВводОстатков</Наименование>
			<Порядок>50</Порядок>
			<Правило Отключить="true">
				<Код>ОстаткиПодарочныхСертификатов</Код>
				<Наименование>ОстаткиПодарочныхСертификатов</Наименование>
				<Порядок>300</Порядок>
				<СпособОтбораДанных>ПроизвольныйАлгоритм</СпособОтбораДанных>
				<ПередОбработкойПравила>
					ДатаПереносаОстатков = НачалоДня(Параметры.ДатаВыгрузкиОстатков)-1;

					Организация = Параметры.ОрганизацияПоУмолчанию;

					ПодарочныеСертификаты = Параметры.ПодарочныеСертификаты;

					СписокСертификатов = Новый Массив;

					Для Каждого ЭлементСоответствия Из ПодарочныеСертификаты Цикл
					СписокСертификатов.Добавить(ЭлементСоответствия.Ключ);
					КонецЦикла;

					Запрос = Новый Запрос;
					Запрос.УстановитьПараметр("Период", ДатаПереносаОстатков);
					Запрос.УстановитьПараметр("Номенклатура", СписокСертификатов);
					Запрос.Текст =
					"ВЫБРАТЬ
					| ТоварыНаСкладахОстатки.Склад КАК Склад,
					| ТоварыНаСкладахОстатки.Номенклатура КАК Номенклатура,
					| ТоварыНаСкладахОстатки.ХарактеристикаНоменклатуры КАК СерийныйНомер,
					| ТоварыНаСкладахОстатки.КоличествоОстаток КАК Количество
					|ИЗ
					| РегистрНакопления.ТоварыНаСкладах.Остатки(&amp;Период, Номенклатура В
					(&amp;Номенклатура)) КАК ТоварыНаСкладахОстатки
					|ГДЕ
					| ТоварыНаСкладахОстатки.КоличествоОстаток &gt; 0
					|ИТОГИ
					| СУММА(КоличествоОстаток)
					|ПО
					| Склад,
					| Номенклатура";

					Если ЗначениеЗаполнено(Параметры.СкладОстатков) Тогда
					Запрос.Текст = СтрЗаменить(Запрос.Текст, "Номенклатура В (&amp;Номенклатура)",
					"Номенклатура В (&amp;Номенклатура) И Склад В (&amp;Склад)");
					Запрос.УстановитьПараметр("Склад", Параметры.СкладОстатков);
					КонецЕсли;

					РезультатЗапроса = Запрос.Выполнить();

					ВыборкаСклад = РезультатЗапроса.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);

					Пока ВыборкаСклад.Следующий() Цикл

					КлючСвязи = 0;

					ДанныеДокумента = Новый Структура;

					ДанныеДокумента.Вставить("Дата", ДатаПереносаОстатков);
					ДанныеДокумента.Вставить("Организация", Организация);
					ДанныеДокумента.Вставить("Магазин", ВыборкаСклад.Склад);
					ДанныеДокумента.Вставить("Склад", ВыборкаСклад.Склад);


					ТаблицаТовары = Новый ТаблицаЗначений;
					ТаблицаТовары.Колонки.Добавить("Номенклатура");
					ТаблицаТовары.Колонки.Добавить("Количество");
					ТаблицаТовары.Колонки.Добавить("КоличествоУпаковок");
					ТаблицаТовары.Колонки.Добавить("Цена");
					ТаблицаТовары.Колонки.Добавить("Сумма", );
					ТаблицаТовары.Колонки.Добавить("КлючСвязиСерийныхНомеров");


					ТаблицаСерийныеНомера = Новый ТаблицаЗначений;
					ТаблицаСерийныеНомера.Колонки.Добавить("КлючСвязиСерийныхНомеров");
					ТаблицаСерийныеНомера.Колонки.Добавить("СерийныйНомер");

					ВыборкаНоменклатура = ВыборкаСклад.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам,
					"Номенклатура");

					Пока ВыборкаНоменклатура.Следующий() Цикл

					КлючСвязи = КлючСвязи + 1;

					СтрокаТаблицы = ТаблицаТовары.Добавить();
					СтрокаТаблицы.Номенклатура = ВыборкаНоменклатура.Номенклатура;
					СтрокаТаблицы.Количество = ВыборкаНоменклатура.Количество;
					СтрокаТаблицы.КоличествоУпаковок = ВыборкаНоменклатура.Количество;
					Номинал = ПодарочныеСертификаты.Получить(СтрокаТаблицы.Номенклатура);
					СтрокаТаблицы.Цена = ?(Номинал = Неопределено, 0, Номинал);
					СтрокаТаблицы.Сумма = СтрокаТаблицы.Цена * СтрокаТаблицы.КоличествоУпаковок;
					СтрокаТаблицы.КлючСвязиСерийныхНомеров = КлючСвязи;

					Выборка = ВыборкаНоменклатура.Выбрать();

					Пока Выборка.Следующий() Цикл

					СтрокаТаблицы = ТаблицаСерийныеНомера.Добавить();
					СтрокаТаблицы.КлючСвязиСерийныхНомеров = КлючСвязи;;
					СтрокаТаблицы.СерийныйНомер = Выборка.СерийныйНомер;

					КонецЦикла;

					КонецЦикла;

					ДанныеДокумента.Вставить("СуммаДокумента", ТаблицаТовары.Итог("Сумма"));
					ДанныеДокумента.Вставить("Товары", ТаблицаТовары);
					ДанныеДокумента.Вставить("СерийныеНомера", ТаблицаСерийныеНомера);
					ВыгрузитьПоПравилу(ДанныеДокумента,,,, "ОстаткиПодарочныхСертификатов");

					КонецЦикла;</ПередОбработкойПравила>
			</Правило>
			<Правило Отключить="true">
				<Код>ОстаткиТоваров</Код>
				<Наименование>ОстаткиТоваров</Наименование>
				<Порядок>350</Порядок>
				<СпособОтбораДанных>ПроизвольныйАлгоритм</СпособОтбораДанных>
				<ПередОбработкойПравила>
					ДатаПереносаОстатков = НачалоДня(Параметры.ДатаВыгрузкиОстатков)-1;

					Организация = Параметры.ОрганизацияПоУмолчанию;

					СписокТоваров = Новый ТаблицаЗначений;
					СписокТоваров.Колонки.Добавить("Номенклатура");
					СписокТоваров.Индексы.Добавить("Номенклатура");

					ПодарочныеСертификаты = Параметры.ПодарочныеСертификаты;

					СписокСертификатов = Новый Массив;

					Для Каждого ЭлементСоответствия Из ПодарочныеСертификаты Цикл
					СписокСертификатов.Добавить(ЭлементСоответствия.Ключ);
					КонецЦикла;

					Запрос = Новый Запрос;
					Запрос.УстановитьПараметр("Период", ДатаПереносаОстатков);
					Запрос.УстановитьПараметр("Номенклатура", СписокСертификатов);
					Запрос.Текст =
					"ВЫБРАТЬ
					| ТоварыНаСкладахОстатки.Склад КАК Склад,
					| ТоварыНаСкладахОстатки.Склад.Представление КАК СкладПредставление,
					| ТоварыНаСкладахОстатки.Номенклатура КАК Номенклатура,
					| ТоварыНаСкладахОстатки.ХарактеристикаНоменклатуры КАК Характеристика,
					| ТоварыНаСкладахОстатки.КоличествоОстаток КАК Количество
					|ИЗ
					| РегистрНакопления.ТоварыНаСкладах.Остатки(&amp;Период, (НЕ Номенклатура В
					(&amp;Номенклатура)) И (Склад &lt;&gt; ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка))) КАК
					ТоварыНаСкладахОстатки
					|
					|ИТОГИ
					|
					|ПО
					| Склад";

					Если ЗначениеЗаполнено(Параметры.СкладОстатков) Тогда
					Запрос.Текст = СтрЗаменить(Запрос.Текст, "(НЕ Номенклатура В (&amp;Номенклатура))", "(НЕ
					Номенклатура В (&amp;Номенклатура)) И Склад В (&amp;Склад)");
					Запрос.УстановитьПараметр("Склад", Параметры.СкладОстатков);
					КонецЕсли;

					РезультатЗапроса = Запрос.Выполнить();

					ВыборкаСклад = РезультатЗапроса.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);

					Пока ВыборкаСклад.Следующий() Цикл

					ДанныеДокумента = Новый Структура;
					ДанныеДокумента.Вставить("Дата", ДатаПереносаОстатков);
					ДанныеДокумента.Вставить("Комментарий", "###Ввод остатков по складу " +
					ВыборкаСклад.СкладПредставление);

					ТаблицаРегистров = Новый ТаблицаЗначений;
					ТаблицаРегистров.Колонки.Добавить("Имя");

					ТаблицаРегистров.Добавить().Имя = "ТоварыНаСкладах";
					ТаблицаРегистров.Добавить().Имя = "ТоварыОрганизаций";
					ДанныеДокумента.Вставить("ТаблицаРегистров", ТаблицаРегистров);


					ТаблицаОстатки = Новый ТаблицаЗначений;
					ТаблицаОстатки.Колонки.Добавить("Организация");
					ТаблицаОстатки.Колонки.Добавить("Склад");
					ТаблицаОстатки.Колонки.Добавить("Номенклатура");
					ТаблицаОстатки.Колонки.Добавить("Характеристика");
					ТаблицаОстатки.Колонки.Добавить("Количество");

					Выборка = ВыборкаСклад.Выбрать();

					Пока Выборка.Следующий() Цикл

					СтрокаТаблицы = ТаблицаОстатки.Добавить();

					ЗаполнитьЗначенияСвойств(СтрокаТаблицы, Выборка);

					СтрокаТаблицы.Организация = Организация;

					СтрокаСписка = СписокТоваров.Найти(Выборка.Номенклатура, "Номенклатура");
					Если СтрокаСписка = Неопределено Тогда
					ЗаполнитьЗначенияСвойств(СписокТоваров.Добавить(), Выборка);
					КонецЕсли;

					КонецЦикла;

					ДанныеДокумента.Вставить("ТоварыНаСкладах", ТаблицаОстатки);
					ДанныеДокумента.Вставить("ТоварыОрганизаций", ТаблицаОстатки);
					ВыгрузитьПоПравилу(ДанныеДокумента,,,, "ОстаткиТоваров");

					КонецЦикла;

					Запрос.УстановитьПараметр("ТипЦен", Параметры.РозничнаяЦена);
					Запрос.УстановитьПараметр("Номенклатура", СписокТоваров);
					Запрос.Текст =
					"ВЫБРАТЬ
					| ЦеныНоменклатурыСрезПоследних.ТипЦен КАК ВидЦены,
					| ЦеныНоменклатурыСрезПоследних.Номенклатура,
					| ЦеныНоменклатурыСрезПоследних.ХарактеристикаНоменклатуры КАК Характеристика,
					| ЦеныНоменклатурыСрезПоследних.Цена,
					| ЦеныНоменклатурыСрезПоследних.ЕдиницаИзмерения КАК Упаковка,
					| ЦеныНоменклатурыСрезПоследних.Валюта.Представление КАК Валюта
					|ИЗ
					| РегистрСведений.ЦеныНоменклатуры.СрезПоследних(
					| &amp;Период,
					| Номенклатура В (&amp;Номенклатура)
					| И ТипЦен = &amp;ТипЦен) КАК ЦеныНоменклатурыСрезПоследних";

					ДанныеДокумента = Новый Структура;
					ДанныеДокумента.Вставить("Дата", ДатаПереносаОстатков);
					ДанныеДокумента.Вставить("Комментарий", "###Цены номенклатуры");

					ТаблицаРегистров = Новый ТаблицаЗначений;
					ТаблицаРегистров.Колонки.Добавить("Имя");

					ТаблицаРегистров.Добавить().Имя = "ЦеныНоменклатуры";
					ДанныеДокумента.Вставить("ТаблицаРегистров", ТаблицаРегистров);
					ДанныеДокумента.Вставить("ЦеныНоменклатуры", Запрос.Выполнить().Выгрузить());
					ВыгрузитьПоПравилу(ДанныеДокумента,,,, "ЦеныНоменклатуры");</ПередОбработкойПравила>
			</Правило>
			<Правило Отключить="true">
				<Код>ПродажиПоДисконтнымКартам</Код>
				<Наименование>ПродажиПоДисконтнымКартам</Наименование>
